    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.lightIndicatorStyleLineWidth": 1,
    "indentRainbow.colors": [
        "rgba(120,255,190,1)",
        "rgba(120,245,255,1)",
        "rgba(120,150,255,1)",
        "rgba(170,120,255,1)",
        "rgba(240,120,255,1)",
        "rgba(255,120,170,1)",
        "rgba(255,120,120,1)"
    ],
    "git.autofetch": true
